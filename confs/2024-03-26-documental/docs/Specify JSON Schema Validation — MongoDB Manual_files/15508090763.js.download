let isDragging = false;
let canBeClicked = true;
let haveSentDragEventToOptly = false;

function alignTheTwoEls() {
  const flag = document.querySelector(".contactUsFlag");
  const blocksContainer = document.querySelector(".contactUsContainer");
  const navbar = document.querySelector('nav[role="navigation"]');
  const maxTop = navbar.getBoundingClientRect().bottom;
  blocksContainer.style.height = "246px";

  if (!flag || !blocksContainer || !navbar) {
    return;
  }

  blocksContainer.style.transition = "none";

  // move container to be centerered with flag
  const flagRect = flag.getBoundingClientRect();
  const containerRect = blocksContainer.getBoundingClientRect();
  const flagCenter = flagRect.top + flagRect.height / 2;

  const rsl = (function () {
    let v1 = flagCenter - containerRect.height / 2 + 20;

    // return Math.max(v1, maxTop) - (blocksContainer.clientHeight - mainBox.clientHeight) + "px";
    return Math.max(v1, maxTop) + "px";
  })();

  blocksContainer.style.top = rsl;
  blocksContainer.style.transition = "0.3s";
}

function makeFlagDraggable() {
  const flag = document.querySelector(".contactUsFlag");

  flag.style.cursor = "grab";

  if (!flag) {
    console.log("no flag found");
    return;
  }

  if (flag.getAttribute("data-draggable") === "true") {
    console.log("flag already draggable");
    return;
  }
  flag.setAttribute("data-draggable", "true");

  let startY = 0;
  let currentY = 0;

  flag.addEventListener("mousedown", function (e) {
    isDragging = true;
    startY = e.clientY;
    currentY = parseInt(getComputedStyle(flag).top);
  });

  document.addEventListener("mousemove", function (e) {
    e.preventDefault();

    if (isDragging) {
      if (canBeClicked) {
        canBeClicked = false;
      }

      if (!haveSentDragEventToOptly) {
        haveSentDragEventToOptly = true;

        window["optimizely"].push({
          type: "event",
          eventName: "WEBXP-298-Contact us-box-dragged",
          tags: {
            revenue: 0, // Optional in cents as integer (500 == $5.00)
            value: 0.0, // Optional as float
          },
        });
      }

      // Move flag
      flag.style.transition = "none";
      const deltaY = e.clientY - startY;
      const newY = currentY + deltaY;

      const navbar = document.querySelector('nav[role="navigation"]');
      const maxTop = navbar.getBoundingClientRect().bottom;
      // Make sure newY is bigger than navbar's bottom

      if (newY < maxTop + 100) {
        return;
      }

      flag.style.top = `${newY}px`;
      flag.style.bottom = "auto";
      flag.style.zIndex = 9999;

      alignTheTwoEls();
    }
  });

  document.addEventListener("mouseup", function () {
    isDragging = false;

    setTimeout(function () {
      canBeClicked = true;
    }, 100);

    flag.style.transition = "all 0.3s";
  });
}

