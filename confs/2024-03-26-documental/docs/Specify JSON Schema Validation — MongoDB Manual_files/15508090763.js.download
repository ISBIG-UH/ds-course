var Experiment = (function () {
  // ========================
  // Data
  // ========================
  var copy = {
    title: "Questions about pricing?",
    subTitle: "Talk to a product specialist about features, sizing, support plans, and consulting.",
    cta: "Chat Now",
  };

  var observer = new MutationObserver(function () {
    var h2s = document.querySelectorAll('[class*="CallToActionButton__Section-sc-"] h2');
    var h4s = document.querySelectorAll('[class*="CallToActionButton__Section-sc-"] h4');
    var buttons = document.querySelectorAll(
      '[class*="CallToActionButton__Section-sc-10fpwuf-"] button[class="btn-green btn-med"]'
    );

    if (h2s.length > 0) {
      h2s.forEach(function (h2) {
        if (h2.innerText !== copy.title) {
          changeH2();
        }
      });
    }

    if (h4s.length > 0) {
      h4s.forEach(function (h4) {
        if (h4.innerText !== copy.subTitle) {
          changeH4();
        }
      });
    }

    if (buttons.length > 0) {
      buttons.forEach(function (button) {
        if (button.innerText !== copy.cta) {
          changeButtonText();
        }
      });
    }

    if (buttons.length > 0) {
      buttons.forEach(function (button) {
        if (button.parentElement.getAttribute("href") !== "javascript:;") {
          changeButtonHref();
        }
      });
    }

    addHiddenButton();
  });

  utils
    .waitForElement('[class*="CallToActionButton__Section-sc-10fpwuf-"]')
    .then(function () {
      var el1 = document.querySelectorAll('[class*="CallToActionButton__Section-sc-10fpwuf-"]')[0];
      var el2 = document.querySelectorAll('[class*="CallToActionButton__Section-sc-10fpwuf-"]')[1];

      var obsSettings = {
        childList: true,
        subtree: true,
      };

      if (!el1 || !el2) return;

      observer.observe(el1, obsSettings);
      observer.observe(el2, obsSettings);
    })
    .catch(function () {});

  function changeH2() {
    utils
      .waitForElement('[class*="CallToActionButton__Section-sc-"] h2')
      .then(function () {
        var els = document.querySelectorAll('[class*="CallToActionButton__Section-sc-"] h2');

        els.forEach(function (el) {
          if (!el) return;
          el.innerText = "";
          el.innerText = copy.title;
        });
      })
      .catch(function () {});
  }

  function changeH4() {
    utils
      .waitForElement('[class*="CallToActionButton__Section-sc-"] h4')
      .then(function () {
        var els = document.querySelectorAll('[class*="CallToActionButton__Section-sc-"] h4');

        els.forEach(function (el) {
          if (!el) return;
          el.innerText = "";
          el.innerText = copy.subTitle;
        });
      })
      .catch(function () {});
  }

  function changeButtonText() {
    utils
      .waitForElement('[class*="CallToActionButton__Section-sc-10fpwuf-"] button[class="btn-green btn-med"]')
      .then(function () {
        var els = document.querySelectorAll(
          '[class*="CallToActionButton__Section-sc-10fpwuf-"] button[class="btn-green btn-med"]'
        );

        els.forEach(function (el) {
          if (!el) return;
          el.innerText = "";
          el.innerText = copy.cta;
        });
      })
      .catch(function () {});
  }

  function changeButtonHref() {
    utils
      .waitForElement('[class*="CallToActionButton__Section-sc-10fpwuf-"] button[class="btn-green btn-med"]')
      .then(function () {
        utils
          .waitUntil(function () {
            return window.Intercom && typeof window.Intercom === "function";
          })
          .then(function () {
            var els = document.querySelectorAll(
              '[class*="CallToActionButton__Section-sc-10fpwuf-"] button[class="btn-green btn-med"]'
            );

            els.forEach(function (el) {
              if (!el) return;
              el.parentElement.removeAttribute("href");
              // el.parentElement.setAttribute("href", "javascript:;");
              // el.parentElement.classList.add("webExpChatTrigger");
              el.parentElement.removeEventListener("click", clickHiddenButton);
              el.parentElement.addEventListener("click", clickHiddenButton);
            });
          })
          .catch(function () {});
      })
      .catch(function () {});
  }

  function addHiddenButton() {
    if (!document.querySelector(".webExpChatTrigger")) {
      var hiddenButton = document.createElement("a");
      hiddenButton.innerHTML = `.`;
      hiddenButton.classList.add("webExpChatTrigger");
      // hiddenButton.style.display = "none";
      hiddenButton.style.width = "10px";
      hiddenButton.style.height = "10px";
      hiddenButton.style.position = "absolute";
      hiddenButton.style.top = "0";
      hiddenButton.style.left = "0";
      hiddenButton.style.zIndex = "-1000000";
      hiddenButton.style.opacity = "0";
      hiddenButton.setAttribute("href", "javascript:;");
      document.body.appendChild(hiddenButton);
    }
  }

  function clickHiddenButton() {
    var hiddenButton = document.querySelector(".webExpChatTrigger");

    if (hiddenButton) {
      hiddenButton.click();
    }
  }

  function start() {
    // Change h2
    changeH2();

    //   Change h4
    changeH4();

    //   Button change text
    changeButtonText();

    // Button change href
    changeButtonHref();

    // Add hidden button
    addHiddenButton();
  }

  return { start };
})();

