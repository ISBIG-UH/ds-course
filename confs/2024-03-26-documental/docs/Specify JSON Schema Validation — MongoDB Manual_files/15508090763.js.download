function runChanges(wrapper){
  var flyoutHTML = `
  <div class="contactUsFlag">
    <div class="content">
      <img src="https://webimages.mongodb.com/_com_assets/cms/ll3vrp28ojl2f8rwk-Expertise.png?auto=format%252Ccompress"/>
      <span>Need help?</span>
    </div>
  </div>
  <div class="contactUsContainer">
    <div class="mainBox">
      <div class="dismiss"><img src="https://webimages.mongodb.com/_com_assets/cms/ll59eeajwj3u0p3m0-Close.png?auto=format%252Ccompress"/></div>
      <h4>Chat with Sales</h4>
      <p>Get more information about features, sizing, support plans, and consulting.</p>
      <div class="webExpChatTrigger " id="webExpChatTrigger">Chat Now</div>
    </div>
    <div class="mainBox">
      <h4>Product Support</h4>
      <p>Having trouble loggin in? Looking to report a bug? We are here to help.</p>
      <div class="webExpSupportTrigger " id="webExpSupportTrigger">Get Help</div>
    </div>
  </div>
  `;
  
  wrapper.insertAdjacentHTML('beforeEnd',flyoutHTML);
  
  let isDragging = false;
  let canBeClicked = true;

  function alignTheTwoEls() {
    const flag = document.querySelector(".contactUsFlag");
    const blocksContainer = document.querySelector(".contactUsContainer");
    const navbar = document.querySelector('nav[role="navigation"],nav');
    const maxTop = navbar.getBoundingClientRect().bottom;
    blocksContainer.style.height = "246px";

    if (!flag || !blocksContainer || !navbar) {
      return;
    }
    
    blocksContainer.style.transition = "none";

    // move container to be centerered with flag
    const flagRect = flag.getBoundingClientRect();
    const containerRect = blocksContainer.getBoundingClientRect();
    const flagCenter = flagRect.top + flagRect.height / 2;

    const rsl = (function () {
      let v1 = flagCenter - containerRect.height / 2 + 20;

      // return Math.max(v1, maxTop) - (blocksContainer.clientHeight - mainBox.clientHeight) + "px";
      return Math.max(v1, maxTop) + "px";
    })();

    blocksContainer.style.top = rsl;
    blocksContainer.style.transition = "0.3s";
  }

  function makeFlagDraggable() {
    const flag = document.querySelector(".contactUsFlag");

    flag.style.cursor = "grab";

    if (!flag) {
      console.log("no flag found");
      return;
    }

    if (flag.getAttribute("data-draggable") === "true") {
      console.log("flag already draggable");
      return;
    }
    flag.setAttribute("data-draggable", "true");

    let startY = 0;
    let currentY = 0;

    flag.addEventListener("mousedown", function (e) {
      isDragging = true;
      startY = e.clientY;
      currentY = parseInt(getComputedStyle(flag).top);
    });

    document.addEventListener("mousemove", function (e) {
      e.preventDefault();

      if (isDragging) {
        if (canBeClicked) {
          canBeClicked = false;
        }

        // Move flag
        flag.style.transition = "none";
        const deltaY = e.clientY - startY;
        const newY = currentY + deltaY;

        const navbar = document.querySelector('nav[role="navigation"],nav');
        const maxTop = navbar.getBoundingClientRect().bottom;
        // Make sure newY is bigger than navbar's bottom

        if (newY < maxTop + 100) {
          return;
        }

        flag.style.top = `${newY}px`;
        flag.style.bottom = "auto";
        flag.style.zIndex = 9999;

        alignTheTwoEls();
      }
    });

    document.addEventListener("mouseup", function () {
      isDragging = false;

      setTimeout(function () {
        canBeClicked = true;
      }, 100);

      flag.style.transition = "all 0.3s";
    });
  }
  
  makeFlagDraggable();
  
  document.querySelector(".contactUsFlag").addEventListener("click", function () {
    if (!canBeClicked) {
      return;
    }

    if (
      !document.querySelector(".contactUsContainer").classList.contains("active") &&
      !document.querySelector(".contactUsFlag").classList.contains("hide")
    ) {
      document.querySelector(".contactUsFlag").classList.add("hide");
      setTimeout(function () {
        document.querySelector(".contactUsContainer").classList.add("active");

        alignTheTwoEls();
      }, 350);
    }

    window.optimizely.push({
      type: "event",
      eventName: "webxp383_flyout_clicks"
    });

  });  

  document.querySelector('.contactUsContainer .dismiss').addEventListener('click',function(){
    if(document.querySelector('.contactUsContainer').classList.contains('active')){
       document.querySelector('.contactUsContainer').classList.add('hide');
       setTimeout(function(){
        document.querySelector('.contactUsContainer').classList.remove('active');
        document.querySelector('.contactUsContainer').classList.remove('hide');
        setTimeout(function(){
          document.querySelector('.contactUsFlag').classList.remove('hide');
        },200);
       }, 325);
      
       if(window.Intercom){
        window.Intercom('hide'); 
       }
    }
  });
  
  var target = document.querySelector('.contactUsContainer');
  var trigger = document.querySelector('.contactUsFlag'); 
  document.addEventListener('click',function(ev){
    var withinBoundaries = ev.composedPath().includes(target);
    var chatTrigger = ev.composedPath().includes(trigger);
    if(!withinBoundaries && !chatTrigger && ev.isTrusted){
      if(document.querySelector('.contactUsContainer').classList.contains('active')){
       document.querySelector('.contactUsContainer').classList.add('hide');
       setTimeout(function(){
        document.querySelector('.contactUsContainer').classList.remove('active');
        document.querySelector('.contactUsContainer').classList.remove('hide');
        setTimeout(function(){
          document.querySelector('.contactUsFlag').classList.remove('hide');
        },200);
       }, 325);
         if(window.Intercom){
          window.Intercom('hide'); 
         }
      }
    }
  });

  document.querySelector('.contactUsContainer .mainBox:nth-of-type(1) .webExpChatTrigger').addEventListener('click',function(){
    window.optimizely.push({
      type: "event",
      eventName: "webxp383_chat_click"
    });
  });

   document.querySelector('.contactUsContainer .mainBox:nth-of-type(2) .webExpSupportTrigger').addEventListener('click',function(){
    window.optimizely.push({
      type: "event",
      eventName: "webxp383_help_clicks"
    });
  });

  
  utils.waitUntil(function(){
    return window.Intercom;
  }).then(function(){
    window.Intercom('onShow', function() {
      document.documentElement.classList.add('intercomButtonShow');
    });
    
    window.Intercom('onHide', function() {
      document.documentElement.classList.remove('intercomButtonShow');
    });      
  });
 
}

utils.waitUntil(function(){
    return document.readyState === 'complete';
  }).then(function(){
    utils.waitForElement('[class*="editor-page__ContentWrapper"]').then(function (wrapper) {
      document.documentElement.classList.add('webxp383');
      runChanges(wrapper);
    });
 });
