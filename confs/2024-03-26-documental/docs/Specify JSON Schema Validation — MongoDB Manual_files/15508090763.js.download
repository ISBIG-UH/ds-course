gs": {}, "integrationStringVersion": 2, "experiments": [{"audienceIds": null, "audienceName": "Everyone else", "changes": null, "id": "27170640139", "integrationSettings": null, "variations": [{"id": "27176480148", "name": "v0 - Original", "actions": []}, {"id": "27170770158", "name": "v1 - Variation #1", "actions": [{"viewId": "22412921891", "changes": [{"id": "269DC62B-B867-48A7-BD0A-A7038F3E6A71", "type": "custom_code", "value": function($){var utils = window.optimizely.get("utils");

var experiment = (function () {
