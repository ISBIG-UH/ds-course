  function setupAdditionalAnimations(){
    // Initial
    function activeTracker(){
      /*
      utils.waitForElement('.choice.active').then(function(activeEl){
        utils.waitForElement('.choiceActiveSlide').then(function(elBorder){
          elBorder.style.height = activeEl.clientHeight + 'px';
          elBorder.style.width = activeEl.clientWidth + 'px';
          elBorder.style.top = activeEl.offsetTop + 'px';
        });
      });
      */      
    }
    
    function updateCards(){
      function forEachWithDelay(array, callback, delay) {
        let i = 0;
        let interval = setInterval(() => {
          callback(array[i], i, array);
          if (++i === array.length) clearInterval(interval);
        }, delay);
      }
      
      document.querySelectorAll('.option.animate__animated').forEach(function(option){
        option.parentNode.classList.remove('overflowEnable');
        option.classList.remove('animate__slideInDownAndLeft');
      });
      
      utils.waitForElement('.block.active').then(function(activeBlock){
        forEachWithDelay(activeBlock.querySelectorAll('.option.animate__animated'), function(option){
          option.classList.add('animate__slideInDownAndLeft');

          option.addEventListener('animationend', function(){
            option.parentNode.classList.add('overflowEnable');
          });

        }, 250);
      });
    }
    
    activeTracker();
    updateCards();

    function scrollIfNeeded(element) {
      const distanceFromTop = element.getBoundingClientRect().top;

      // If the top of the element is outside the viewport or hidden behind
      // the sticky header, scroll to the top of the element.
      if (distanceFromTop <= 100 || distanceFromTop >= window.innerHeight) {
        element.scrollIntoView();
      }
    }

    // Update
    var choices = document.querySelectorAll('.expComponent .choice');    
    choices.forEach(function(choice){
      choice.addEventListener('click',function(){
        activeTracker();
        updateCards();
        if(document.querySelector('.expComponent h3')){
          scrollIfNeeded(document.querySelector('.expComponent h3'));
        }
      });
    });
    
    // iOS triggers resizes on scrolling potentially
    var defaultWidth = document.documentElement.offsetWidth;

    window.addEventListener("resize", debounce(function () {
      activeTracker();
      
      // Reset active option for mobile devices
      if(document.documentElement.offsetWidth <= 768 && document.documentElement.offsetWidth !== defaultWidth){
        document.querySelector('.select-items div:nth-of-type(1)').click();
        defaultWidth = document.documentElement.offsetWidth;
      }
    },100));
       
  }
  
  function setupListeners(){
