    var faqWrapperOuter = document.createElement("div");
    var faqContainer = document.createElement("div");
    faqContainer.classList.add("exp-w-wrapper");
    faqContainer.classList.add("exp-faq-container");
    faqWrapperOuter.classList.add("exp-faq-bg");

    faqWrapperOuter.appendChild(faqContainer);

    faqContainer.innerHTML = `
      <div>
        <p class="exp-faq-title">Frequently asked questions</p>
      </div>
      <div class="exp-faq-subitle">
        <p class="exp-faq-description">Check out some of our most frequently asked questions.</p>
      <div>
      
      <div class="exp-faq-divider"></div>
    
    `;

    data.FAQs.forEach(function (faq) {
      var faqEl = createFaq(faq);
      faqContainer.appendChild(faqEl);
    });

    placeAfter(faqWrapperOuter, document.querySelector(".fl.fl-wrap.fl-justify-evenly.w-full"));

    var bottomCards = createBottomCards();
    placeAfter(bottomCards, document.querySelector(".exp-faq-wrapper:last-of-type"));
  }

  function createFaq(faq) {
    var faqEl = document.createElement("div");
    faqEl.setAttribute("data-exp-id", faq.id);
    faqEl.classList.add("exp-w-wrapper");
    faqEl.classList.add("exp-faq-wrapper");

    faqEl.innerHTML = `
      <div>
        <div class="faq-title-wrapper">
          <p class="exp-faq-question">${faq.question}</p>
          <img class="exp-faq-icon-plus" src="${data.plusIcon}"/>
          <img class="exp-faq-icon-minus" src="${data.minusIcon}"/>
        </div>

        <div class="exp-faq-answer-el">
          <p class="exp-faq-answer">${faq.answer}</p>
        </div>
      </div>
    `;

    return faqEl;
  }

