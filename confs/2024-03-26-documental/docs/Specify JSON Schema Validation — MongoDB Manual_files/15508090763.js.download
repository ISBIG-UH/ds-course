  function additionalValidation(){
    utils.waitForElement('#node-form mdb-button').then(function(submitCTA) {
        window.triggeredCTAChanges = true;
        var clone = submitCTA.cloneNode(true);
        clone.classList.add('clonedButton');
        submitCTA.style.display = 'none';
        submitCTA.classList.add('originalButton');
        submitCTA.insertAdjacentElement('afterEnd',clone);

        function removeCommentErrors(){
          if(document.querySelector('.comment-error')){
              document.querySelector('.comment-error').remove();
          }
          comments.shadowRoot.querySelector("label").classList.remove('exp-invalid');
        }

        // Focus out is dangerous because the user can immediately click the CTA and focusOut will remove the error message.
        document.getElementById('comments').addEventListener('change',function(){

          // New Request
          var comments = document.getElementById("comments");
          var wordSplit = comments.value.split(/\s+/);

          var wordTest = /\b\w+\b(?:.*?\b\w+\b){4}/.test(comments.value);

          var errorReason;
          var valid = true;

          // 5 words min
          if(!wordTest){
            valid = false;
            errorReason = 'Comments should be at least five words.';
          }

          // Disallow numbers-only
          if((/^\d+\.?\d*$/).test(comments.value)){
            valid = false;
            errorReason = 'Comments can not be only numbers.';
          }

          // Disallow script tags
          if(comments.value.indexOf('<script') > -1){
            valid = false;
            errorReason = 'Comments can not contain script tags.';
          }

          // Disallow img tags
          if(comments.value.indexOf('<img') > -1){
            valid = false;
            errorReason = 'Comments can not contain img tags.';
          }

          if(document.querySelector('.comment-error')){
            document.querySelector('.comment-error').innerHTML = errorReason;
            // Presumably this is still attached
            comments.shadowRoot.querySelector("label").classList.add('exp-invalid');
          }else{
            comments.insertAdjacentHTML('afterEnd',`<div class="error-msg comment-error">${errorReason}</div>`);
            comments.shadowRoot.querySelector("label").classList.add('exp-invalid');
          }

          if(valid){
            removeCommentErrors();      
          }
        });
        

        clone.addEventListener('click', function(e) {
          e.preventDefault();
          // Required inputs
          var form = document.querySelector('#node-form');
          var emailInput = form.querySelector('#email');
          var firstNameInput = form.querySelector('#first_name');
          var lastNameInput = form.querySelector('#last_name');

          var companyInput = form.querySelector('#company');
          var phoneInput = form.querySelector('#phone');
          var jobInput = form.querySelector('#job_function');

          if (!emailInput.reportValidity()) {
              emailInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          if (!firstNameInput.reportValidity()) {
              firstNameInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          if (!lastNameInput.reportValidity()) {
              lastNameInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          if (!companyInput.reportValidity()) {
              companyInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          if (!phoneInput.reportValidity()) {
              phoneInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          if (!jobInput.reportValidity()) {
              jobInput.checkValidity();
              // document.documentElement.scrollIntoView('#node-form')
              return false;
          }

          // New Request
          var comments = document.getElementById("comments");
          //var wordSplit = comments.value.split(/\s+/);
          var wordTest = /\b\w+\b(?:.*?\b\w+\b){4}/.test(comments.value);

          var errorReason;
          var valid = true;

          // 5 words min
          if(!wordTest){
            valid = false;
            errorReason = 'Comments should be at least five words.';
          }


          // Disallow numbers-only
          if((/^\d+\.?\d*$/).test(comments.value)){
            valid = false;
            errorReason = 'Comments can not be only numbers.';
          }

          // Disallow script tags
          if(comments.value.indexOf('<script') > -1){
            valid = false;
            errorReason = 'Comments can not contain script tags.';
          }

          // Disallow img tags
          if(comments.value.indexOf('<img') > -1){
            valid = false;
            errorReason = 'Comments can not contain img tags.';
          }

          if(valid){
            submitCTA.click();
          }else{
            if(document.querySelector('.comment-error')){
              document.querySelector('.comment-error').innerHTML = errorReason;
            }else{
              comments.insertAdjacentHTML('afterEnd',`<div class="error-msg comment-error">${errorReason}</div>`);
            }
            comments.shadowRoot.querySelector("label").classList.add('exp-invalid');
          }
        });

    });

  }

