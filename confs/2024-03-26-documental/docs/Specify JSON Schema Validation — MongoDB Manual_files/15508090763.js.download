function observeElement(cssSelector, callback, condition) {
  // Checks
  if (!callback || typeof callback !== "function") {
    throw new Error("Callback must be a function");
  }

  if (!condition || typeof condition !== "function") {
    throw new Error("Callback must be a function");
  }

  if (!cssSelector || typeof cssSelector !== "string") {
    throw new Error("CSS selector must be a string");
  }

  if (!window[`observe-${cssSelector}`]) {
    const el = document.querySelector(cssSelector);
    if (el) {
      const config = {
        attributes: true,
        childList: true,
        subtree: true,
      };
      callback();

      const callbackWrapper = (mutationsList, observer) => {
        for (const mutation of mutationsList) {
          if (condition()) {
            callback();
          }
        }
      };

      window[`observe-${cssSelector}`] = new MutationObserver(callbackWrapper);
      window[`observe-${cssSelector}`].observe(el, config);
    }
  }
}

