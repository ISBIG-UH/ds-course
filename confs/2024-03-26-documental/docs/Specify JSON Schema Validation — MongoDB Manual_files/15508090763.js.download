  var sectionsHeights = {};

  // ===============================================================
  // Helpers
  // ===============================================================
  function placeAfter(newNode, referenceNode) {
    if (!newNode) {
      throw new Error("newNode is required");
    }

    if (!referenceNode) {
      throw new Error("referenceNode is required");
    }

    referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
  }

  function addCss(css, id) {
    if (!css || document.getElementById(id)) return;

    var style = document.createElement("style");
    style.id = id;
    style.innerHTML = css;
    document.getElementsByTagName("head")[0].appendChild(style);
  }

  window.expScrollToId = function (id) {
    if (!id) return;

    var el = document.getElementById(id);
    if (!el) return;

    el.scrollIntoView();
  };

  // ===============================================================
  // Main
  // ===============================================================

  function setActiveEl(elId) {
    var el = document.querySelector(`[data-exp-id="${elId}"]`);
    if (!el) return;

    if (document.querySelector(".exp-active-menu-item")) {
      document.querySelectorAll(".exp-active-menu-item").forEach(function (el) {
        el.classList.remove("exp-active-menu-item");
      });
    }
    
    el.classList.add("exp-active-menu-item");
    
  }

  function showFloatingBlock() {
    // Find .exp-secondary-right
    var secondaryRight = document.querySelector(".exp-secondary-right");
    // Find where to place the floating block
    var secondaryRightRect = secondaryRight.getBoundingClientRect();

    var top = secondaryRightRect.top + secondaryRightRect.height + 5;
    var right = window.innerWidth - secondaryRightRect.right;

    //   Floating block css
    addCss(
      `
    .exp-floating-block-wrapper {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: transparent;
        z-index: 9999;
    }

    .exp-floating-block {
        padding: 40px;
        background: #F5F7FA;
        position: absolute;
        border-radius: 12px;
        top: ${top}px;
        right: ${right}px;
    }

    .exp-floating-block-item {
        font-family: 'Euclid Circular A';
        font-style: normal;
        font-weight: 400;
        font-size: 16px;
        line-height: 24px;
        color: #001E2B;
        position: relative;
        margin: 0;
        margin-right: 20px;
        padding: 5px 0;
    }

    .exp-floating-block-item a {
        color: #001E2B;
        text-decoration: none;
    }

    .exp-li-active::after {
        content: '';
        position: absolute;
        top: 50%;
        right: -28px;
        transform: translateY(-50%);
        width: 16px;
        height: 16px;
        background-image: url(${checkmarkUrl});
        background-repeat: no-repeat;
        background-size: contain;
    }

    .exp-floating-block-item:hover {
        cursor: pointer;
        font-family: 'Euclid Circular A';
        font-style: normal;
        font-weight: 500;
        font-size: 16px;
        line-height: 24px;
        color: #001E2B;
    }

    .exp-floating-block-wrapper-mobile {
        position: sticky;
        top: 0;
        z-index: 9999;
        width: 100%;
        height: 48px;
    }

    @media (min-width: 820px) {
        .exp-floating-block-wrapper-mobile {
            display: none;
        }
    }

    
    @media (max-width: 820px) {
        .exp-floating-block {
            display: block;
            width: 100%;
            left: 0;
            right: 0;
            margin-top: -12px;
            border-radius: 0 0 12px 12px;
            padding: 32px;
        }

        .exp-floating-block-item {
            padding: 15px 0px;
        }

        .exp-li-active a {
            color: #00684A;
        }

        .exp-li-active::after {
            display: none;
        }
    }
